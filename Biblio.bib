issn = {23524316},
journal = {Extreme Mechanics Letters},
keywords = {Elastomer,Exoskeleton,High force,Rotational casting,Soft robot},
pages = {89--104},
publisher = {Elsevier Ltd},
title = {{Scalable manufacturing of high force wearable soft actuators}},
