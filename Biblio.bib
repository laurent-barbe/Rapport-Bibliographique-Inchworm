file = {:D$\backslash$:/Loic{\_}Mosser/2{\_}20{\_}StageMosser2020/2{\_}20{\_}StageMosser2020/BiblioMosser/Techniques ingenieur/fab/schmitt2018.pdf:pdf},
issn = {22969144},
journal = {Frontiers Robotics AI},
keywords = {Design,Manufacturing process,Prototyping,Soft components,Soft robotics},
number = {JUN},
title = {{Soft robots manufacturing: A review}},
volume = {5},
year = {2018}
}
